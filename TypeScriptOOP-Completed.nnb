        {
            "language": "typescript",
            "source": [
                "\nclass Duck {\n  public static className: string = 'Duck';\n  \n  constructor(protected age$:number){}\n  \n  get age():number{\n      return this.age$\n  }\n  \n  public quack():void{\n      console.log('Quack!')\n  }\n  \n  public swim():void{\n      console.log('Paddles with two feet')\n  }\n}\n\n\nclass MallardDuck extends Duck {\n  public static className:string = 'MallardDuck'\n  \n  constructor(public color:string, age){\n      super(age)\n  }\n  \n  // Let's pretend that all Mallard Ducks are always 5 years older than they claim\n  public get age():number{\n      return this.age$ + 5\n  }\n  \n  // Let's also override the quack method to have a mallard's quack\n  public quack():void{\n      console.log('Mallard Quack!!')\n  }\n}"
