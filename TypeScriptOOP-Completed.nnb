        {
            "language": "typescript",
            "source": [
                "// Main Interface - Quack, Swim, Fly\ninterface Quackable {\n    quack():void\n}\n\ninterface Swimmable {\n    swim():void\n}\n\ninterface Flyable {\n    fly():void\n    land():void\n}\n\n\n// Set up classes for different ways of implementing the interface\n// Quacks - 3 types of Quacking - Quack, Sqeak, Muted\n\nclass Quacks implements Quackable{\n    quack():void{\n        console.log('Quack!')\n    }\n}\n\nclass Squeaks implements Quackable{\n    quack():void{\n        console.log('Sqeak!')\n    }\n}\n\nclass Muted implements Quackable{\n    quack():void{\n        console.log('...')\n    }\n}\n\n// Swim - 2 types of Swimming - Swim, Float\n\nclass Swims implements Swimmable {\n    swim():void{\n        console.log('Swimming with two little feet')\n    }\n}\n\nclass Floats implements Swimmable {\n    swim():void{\n        console.log('Floating around')\n    }\n}\n\n// Fly - 2 types of Flying - FlyWithWings, NeverFly\nclass FlyWithWings implements Flyable {\n    fly():void{\n        console.log('Flying high through the sky')\n    }\n    \n    land():void{\n        console.log('Safely landing on the ground')\n    }\n}\n\n\nclass NeverFlys implements Flyable {\n    fly():void{\n        console.log('Still on the ground... :(')\n    }\n    \n    land():void{\n        console.log('Never took off in the first place...')\n    }\n}\n\n\n// Create the abstract Duck class that implements the interfaces and has properties for the different\n// interface implementations\nabstract class Duck implements Quackable, Swimmable, Flyable {\n    protected quackAbility$: Quackable\n    protected swimAbility$: Swimmable\n    protected flyAbility$: Flyable\n    \n    constructor(protected age$:number){}\n    \n    get age():number{\n        return this.age$\n    }\n    \n    set quackAbility(qa:Quackable){\n        this.quackAbility$ = qa\n    }\n    \n    set swimAbility(sa:Swimmable){\n        this.swimAbility$ = sa\n    }\n    \n    set flyAbility(fa:Flyable){\n        this.flyAbility$ = fa\n    }\n    \n    quack():void{\n        this.quackAbility$.quack()\n    }\n    \n    swim():void{\n        this.swimAbility$.swim()\n    }\n    \n    fly():void{\n        this.flyAbility$.fly()\n    }\n    \n    land():void{\n        this.flyAbility$.land()\n    }\n}\n\n\n// Create classes that inherit from Duck\nclass MallardDuck extends Duck{\n    quackAbility$: Quackable = new Quacks()\n    swimAbility$: Swimmable = new Swims()\n    flyAbility$: Flyable = new FlyWithWings()\n}\n\nclass RedHeadedDuck extends Duck{\n    quackAbility$: Quackable = new Quacks()\n    swimAbility$: Swimmable = new Swims()\n    flyAbility$: Flyable = new FlyWithWings()\n}\n\nclass DecoyDuck extends Duck{\n    quackAbility$: Quackable = new Muted()\n    swimAbility$: Swimmable = new Floats()\n    flyAbility$: Flyable = new NeverFlys()\n    \n}\n\nclass RubberDuck extends Duck{\n    quackAbility$: Quackable = new Squeaks()\n    swimAbility$: Swimmable = new Floats()\n    flyAbility$: Flyable = new NeverFlys()\n}"
            ],
